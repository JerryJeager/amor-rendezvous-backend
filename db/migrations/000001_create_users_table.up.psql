CREATE TABLE users(
    id           UUID PRIMARY KEY,
    email        VARCHAR(100) NOT NULL,
    first_name   VARCHAR(100) NOT NULL,
    last_name    VARCHAR(100) NOT NULL,
    password     VARCHAR(100) NOT NULL,
    is_to_wed    BOOLEAN,
    created_at   TIMESTAMPTZ NOT NULL,
    updated_at   TIMESTAMPTZ,
    deleted_at   TIMESTAMPTZ

);